mixin symbolCard()
    each sym in StockData

        .main-card
            div.card-comp-info 
                p=sym.sym
                p=sym.name
            .time-frame-card 
                .top-time-frame 
                    div 
                        p 1 week
                    
                    div 
                        p 2 week 
                div 
                    p 1 month



