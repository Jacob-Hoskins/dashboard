mixin symbol
    each sym in StockSym
        .symbol-data
            p
                a(href=`/stocks/${sym}`, class="symbol")=sym
            

            .symbol-params
                //- 1 week
                //- RSI 
                if num <= 30
                    p.pass-test &#x2713
                else  
                    p.fail-test X
                
